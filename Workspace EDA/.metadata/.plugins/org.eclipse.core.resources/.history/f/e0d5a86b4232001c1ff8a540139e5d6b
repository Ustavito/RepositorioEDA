package mapas;

public class Alumno {
	private String nombre;
	private String dni;
	private int edad;
	
	@Override
	public boolean equals (Object obj) {
		
		boolean iguales = false;

        if (obj instanceof Alumno) {
        	Alumno alumno = (Alumno) obj;
        	iguales = (this.nombre == alumno.nombre) && (this.dni == alumno.dni);
        } 
        
        return iguales;
    }
}